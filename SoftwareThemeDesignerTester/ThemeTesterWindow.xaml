<Window x:Class="SoftwareThemeDesignerTester.ThemeTesterWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="MainWindow"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:viewModels="clr-namespace:SoftwareThemeDesignerTester.ViewModels"
		xmlns:controls="clr-namespace:ReliaCoat.Common.UI.Controls;assembly=ReliaCoat.Common.UI.Controls"
		mc:Ignorable="d"
		d:DataContext="{d:DesignInstance viewModels:ThemeTesterWindowViewModel}"
		WindowStartupLocation="CenterScreen"
		Background="{DynamicResource Background}"
		Height="700"
		Width="1400">

	<DockPanel>
		
		<controls:RctComboCheckList checkListItemsSource="{Binding countryRepository}"
									checkedItems="{Binding countryList, Mode=TwoWay}"
									groupMember="currency"
									displayMember="name"
									Margin="10"/>

	</DockPanel>


</Window>